<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:strings="clr-namespace:Soundchaser.TagTools.Maui.Resources.Strings"
             x:Class="Soundchaser.TagTools.Maui.Views.PlaylistsPage"
             Title="{x:Static strings:AppStrings.PlaylistsPageTitle}">
    <VerticalStackLayout Padding="16">
        <Label Text="{x:Static strings:AppStrings.PlaylistsPageTitle}" FontSize="24" FontAttributes="Bold" />
        <Label Text="{x:Static strings:AppStrings.PlaylistsComingSoon}" Margin="0,8,0,0" />
    </VerticalStackLayout>
</ContentPage>
